{
  "title": "[YJ Park] Launch apps",
  "rules": [
    {
      "description": "Launch Apps with Left Control, Left Cmd + Letters",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "osascript -e 'tell app \"iTerm2\"' -e 'create window with default profile' -e activate -e end"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "left_control", "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Atom.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_control", "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Chrome.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "left_control", "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Safari.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "left_control", "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Visual Studio Code.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "left_control", "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Unity.app'"
            }
          ]
        }
      ]
    }
  ]
}
